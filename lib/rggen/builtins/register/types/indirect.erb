rggen_indirect_register #(
  .ADDRESS_WIDTH    (<%= local_address_width %>),
  .START_ADDRESS    (<%= start_address %>),
  .END_ADDRESS      (<%= end_address %>),
  .INDEX_WIDTH      (<%= indirect_index_width %>),
  .INDEX_VALUE      (<%= indirect_index_value %>),
  .DATA_WIDTH       (<%= data_width %>),
  .TOTAL_BIT_FIELDS (<%= total_bit_fields %>),
  .MSB_LIST         (<%= msb_list %>),
  .LSB_LIST         (<%= lsb_list %>)
) u_<%= register.name %> (
  .register_if  (<%= register.register_if %>),
  .bit_field_if (<%= bit_field_if[loop_variables] %>),
  .i_index      (<%= indirect_index[loop_variables] %>)
);
